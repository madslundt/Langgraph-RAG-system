# LLM Provider Configuration
# Choose your preferred LLM provider and add the corresponding API key (if needed)

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Default LLM Settings
DEFAULT_LLM_PROVIDER=ollama  # Options: openai, anthropic, ollama
DEFAULT_MODEL=qwen3:8b

# Embedding Configuration
EMBEDDING_PROVIDER=ollama        # Options: openai, huggingface, ollama
EMBEDDING_MODEL=mxbai-embed-large

# Chunking Configuration (optional - can be overridden in config)
CHUNK_SIZE=500
CHUNK_OVERLAP=100

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR
